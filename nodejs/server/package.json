{
  "name": "nodejs-benchmark",
  "version": "1.0.0",
  "description": "Node-Generated built with node + typescript + react",
  "author": "",
  "license": "MIT",
  "scripts": {
    "start:dev": "CLOUDSDK_CORE_PROJECT=node-generated-development npx server",
    "format": "prettier --write \"./src/**/*.ts\" && prettier --write \"./src/**/*.graphqls\"",
    "lint": "tslint --project .",
    "start": "node ./dist/main.js",
    "prestart:dev": "rm -rf dist/",
    "start:debug": "ts-node --inspect src/main.ts",
    "compile:prod": "rm -rf dist && tsc",
    "test": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "build": "rm -rf public && npm run compile:prod",
    "deploy:dev": "CLOUDSDK_CORE_PROJECT=node-generated-dev npm run deploy && TAG_NAME=dev/`date +%Y%m%d_%H%M%S` && git tag -a $TAG_NAME -m 'dev deployed' && git push origin $TAG_NAME",
    "deploy:uat": "CLOUDSDK_CORE_PROJECT=node-generated-uat npm run deploy && TAG_NAME=uat/`date +%Y%m%d_%H%M%S` && git tag -a $TAG_NAME -m 'uat deployed' && git push origin $TAG_NAME",
    "deploy:prod": "CLOUDSDK_CORE_PROJECT=node-generated-prod npm run deploy && TAG_NAME=prod/`date +%Y%m%d_%H%M%S` && git tag -a $TAG_NAME -m 'prod deployed' && git push origin $TAG_NAME",
    "deploy": "npm run build && gcloud app deploy cron.yaml index.yaml app.yaml",
    "dsui": "dsui --port 4000",
    "kill": "bash ./scripts/kill.sh",
    "kill-force": "bash ./scripts/kill.sh -9",
    "clear-datastore": "rm -rf ~/.config/gcloud/emulators/datastore"
  },
  "dependencies": {
    "@3wks/gae-node-nestjs": "1.6.1",
    "@google-cloud/connect-datastore": "2.0.1",
    "@google-cloud/datastore": "3.1.0",
    "@google-cloud/debug-agent": "3.0.1",
    "@google-cloud/logging-bunyan": "0.9.5",
    "@google-cloud/storage": "2.4.0",
    "@google-cloud/trace-agent": "3.5.2",
    "@nestjs/common": "5.6.2",
    "@nestjs/core": "5.6.2",
    "@nestjs/testing": "5.6.2",
    "bcryptjs": "2.4.3",
    "bunyan": "1.8.12",
    "colors": "1.3.3",
    "config": "3.0.1",
    "csurf": "1.9.0",
    "csvtojson": "2.0.8",
    "dataloader": "1.4.0",
    "ejs": "2.6.1",
    "email-addresses": "3.0.3",
    "faker": "4.1.0",
    "fastify-formbody": "2.1.0",
    "fetch": "1.1.0",
    "googleapis": "37.0.0",
    "graphql": "14.1.1",
    "graphql-tools": "4.0.4",
    "html-pdf": "2.2.0",
    "io-ts": "1.7.1",
    "io-ts-reporters": "0.0.21",
    "jsonwebtoken": "8.4.0",
    "lodash": "4.17.11",
    "merge-graphql-schemas": "1.5.8",
    "node-fetch": "2.3.0",
    "node-uuid": "1.4.8",
    "nodemailer": "5.1.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.3.3",
    "tslib": "1.9.3",
    "twilio": "3.27.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@streamrail/dsui": "0.2.2",
    "@types/bcryptjs": "2.4.2",
    "@types/bunyan": "1.8.5",
    "@types/config": "0.0.34",
    "@types/csvtojson": "1.1.5",
    "@types/ejs": "2.6.1",
    "@types/express": "4.16.1",
    "@types/express-session": "1.15.11",
    "@types/faker": "4.1.5",
    "@types/google-cloud__datastore": "^1.3.2",
    "@types/google-cloud__storage": "1.7.2",
    "@types/graphql": "14.0.5",
    "@types/html-pdf": "2.1.2",
    "@types/jest": "23.3.13",
    "@types/jsonwebtoken": "8.3.0",
    "@types/lodash": "4.14.120",
    "@types/node": "10.12.19",
    "@types/node-fetch": "2.1.4",
    "@types/node-uuid": "0.0.28",
    "@types/nodemailer": "4.6.5",
    "@types/passport": "1.0.0",
    "@types/passport-google-oauth": "1.0.39",
    "@types/passport-local": "1.0.33",
    "@types/passport-saml": "1.0.0",
    "@types/supertest": "2.0.7",
    "@types/twilio": "0.0.10",
    "@types/uuid": "3.4.4",
    "chokidar": "2.0.4",
    "clean-webpack-plugin": "1.0.0",
    "concurrently": "4.1.0",
    "jest": "23.6.0",
    "nodemon": "1.18.9",
    "prettier": "1.16.1",
    "supertest": "3.4.2",
    "ts-jest": "23.10.5",
    "ts-loader": "5.3.3",
    "ts-mockito": "2.3.1",
    "ts-node": "8.0.2",
    "tsconfig-paths": "3.7.0",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "typescript": "3.3.3",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1",
    "webpack-node-externals": "1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
